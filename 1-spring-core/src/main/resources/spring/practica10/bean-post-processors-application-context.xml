<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Define bean workerBean -->
	<bean init-method="init" class="org.certificatic.spring.core.practica10.beanpostprocessors.bean.Worker">
		<property name="age" value="23"></property>
		<property name="name" value="Elsa"></property>
	</bean>

	<!-- Define bean clase BeanPostProcessor1 -->
	<bean class="org.certificatic.spring.core.practica10.beanpostprocessors.bpp.BeanPostProcessor1"></bean>

	<!-- Define bean clase BeanPostProcessor2 -->
	<bean class="org.certificatic.spring.core.practica10.beanpostprocessors.bpp.BeanPostProcessor2"></bean>

	<!-- Define bean clase BeanPostProcessor3 -->
	<bean class="org.certificatic.spring.core.practica10.beanpostprocessors.bpp.BeanPostProcessor3">
	</bean>
</beans>
