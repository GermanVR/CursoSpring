<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.2.xsd">

	<!-- Define bean clase Car -->
	<bean class="org.certificatic.spring.core.practica13.autowire.bean.Car" scope="prototype"
		autowire="byName"></bean>

	<!-- Define bean engine -->
	<bean id="engine" class="org.certificatic.spring.core.practica13.autowire.bean.Engine" scope="prototype">
		<property name="cylinderNumber" value="360"></property>
		<property name="model" value="Ferra"></property>
		<property name="serialNumber" value="360-ABXC"></property>
	</bean>

	<!-- Define bean wheelGroup -->
	<bean id="wheelGroup" class="org.certificatic.spring.core.practica13.autowire.bean.WheelGroup"
		scope="prototype" autowire="byName" />
		
	<!-- Define lista wheelList -->
	<util:list id="wheelList" list-class="java.util.ArrayList" scope="prototype">
		<ref bean="wheel" />
		<ref bean="wheel" />
		<ref bean="wheel" />
		<ref bean="wheel" />
	</util:list>


	<!-- Define bean wheel -->
	<bean p:material="Caucho con spandex" p:speedIndicator="250 75 y" p:rinSize="22" id="wheel"
		class="org.certificatic.spring.core.practica13.autowire.bean.Wheel" scope="prototype">

	</bean>
</beans>
